import matplotlib.pyplot as plt
import numpy as np

def PokemonMatchups():
    x = np.linspace(0, 2 * np.pi, 200)
    y = np.sin(x)

    fig, ax = plt.subplots()
    ax.plot(x, y)
    plt.show()


PokemonMatchups()